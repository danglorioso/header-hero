{
	"name": "header-hero",
	"displayName": "Header Hero - Quick Headers and Function Documentation",
	"description": "Automatically insert customizable headers and function contracts into your code, making documentation quick and effortless. Improve your code's readability and maintainability with just a click.",
	"version": "1.0.0",
	"publisher": "DanGlorioso",
	"icon": "icon.png",
	"repository": {
		"type": "git",
		"url": "https://github.com/danglorioso/header-hero"
	},
	"engines": {
		"vscode": "^1.73.0"
	},
	"categories": [
		"Other"
	],
	"contributes": {
		"commands": [
			{
				"command": "headerhero.checkfunctionlength",
				"title": "Check Function Length of All Functions in This File"
			}
		]
	},
	"activationEvents": [
		"onCommand:extension.insertHeader",
		"onCommand:extension.insertFunctionContract"
	],
	"main": "./out/extension.js",
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint \"src/**/*.ts\"",
		"watch": "tsc -watch -p ./"
	},
	"devDependencies": {
		"@types/vscode": "^1.73.0",
		"@typescript-eslint/eslint-plugin": "^6.7.0",
		"@typescript-eslint/parser": "^6.7.0",
		"eslint": "^8.26.0",
		"typescript": "^5.4.2"
	},
	"dependencies": {
		"nvm": "^0.0.4"
	}
}
